{
  "name": "express-modular-api",
  "version": "1.0.0",
  "main": "./src/index.js",
  "license": "MIT",
  "type": "commonjs",
  "scripts": {
    "reset": "npx cross-env rm -rf ./node_modules && yarn",
    "table:status": "npx knex migrate:status",
    "table:clear": "npx knex migrate:rollback",
    "table:build": "npx knex migrate:latest",
    "table:up": "npx knex migrate:up",
    "table:down": "npx knex migrate:down",
    "swagger": "npx ts-node ./swagger.ts",
    "dev": "npx nodemon",
    "lint": "npx eslint ./src --ext .ts",
    "lint:fix": "npx eslint ./src --ext .ts --fix",
    "prettier": "npx prettier --config .prettierrc \"./src/**/*.ts\" --write",
    "build": "npx rimraf ./build && npx tsc",
    "start": "node ./build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "_moduleAliases": {
    "@/configs": "build/configs",
    "@/environments": "build/environments",
    "@/paths": "build/paths",
    "@/apps": "build/apps",
    "@/contracts": "build/contracts",
    "@/helpers": "build/helpers",
    "@/middlewares": "build/middlewares",
    "@/models/repositories": "build/models/repositories",
    "@/utils": "build/utils"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "helmet": "^7.0.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.3.1",
    "node-cron": "^3.0.2",
    "swagger-autogen": "^2.23.1",
    "swagger-ui-express": "^4.6.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-fileupload": "^1.4.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/knex": "^0.16.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.3.3",
    "@types/node-cron": "^3.0.8",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "module-alias": "^2.2.3",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "yarn": "^1.22.19"
  }
}
